
   #include<stdio.h>
   //problem: digit sum with function declartion 

   int sum (int num1 ,int num2);

   int main()
   {
   int a ,b;
   scanf("%d %d",&a,&b);

   int result =sum(a,b);
   printf("sum of %dand %d is %d ",a,b,result);

   return 0;
   }
   int sum(int num1 ,int num2)
   {
      int val= num1+num2;
      return val;
   }
